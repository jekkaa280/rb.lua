    pursuitPlusButton.Parent = pursuitSpeedFrame
    
    local function updatePursuitSpeedDisplay()
        pursuitSpeedLabel.Text = tostring(pursuitSpeed)
        
        if pursuitSpeed <= 10 then
            pursuitSpeedLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        elseif pursuitSpeed <= 20 then
            pursuitSpeedLabel.TextColor3 = Color3.fromRGB(255, 200, 100)
        elseif pursuitSpeed <= 40 then
            pursuitSpeedLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        else
            pursuitSpeedLabel.TextColor3 = Color3.fromRGB(0, 255, 200)
        end
    end
    
    pursuitMinusButton.MouseButton1Click:Connect(function()
        pursuitSpeed = math.max(1, pursuitSpeed - 5)
        updatePursuitSpeedDisplay()
        print("[Pursuit] –°–∫–æ—Ä–æ—Å—Ç—å —É–º–µ–Ω—å—à–µ–Ω–∞: " .. pursuitSpeed)
    end)
    
    pursuitPlusButton.MouseButton1Click:Connect(function()
        pursuitSpeed = math.min(100, pursuitSpeed + 5)
        updatePursuitSpeedDisplay()
        print("[Pursuit] –°–∫–æ—Ä–æ—Å—Ç—å —É–≤–µ–ª–∏—á–µ–Ω–∞: " .. pursuitSpeed)
    end)
    
    updatePursuitSpeedDisplay()
end

-- ========== –£–ü–†–ê–í–õ–ï–ù–ò–ï –ú–ï–ù–Æ ==========
local function toggleMenu()
    MainFrame.Visible = not MainFrame.Visible
    if MainFrame.Visible then
        openButton.Text = "üì± CLOSE"
        openButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
        closeTargetList()
    else
        openButton.Text = "üì± MENU"
        openButton.BackgroundColor3 = Color3.fromRGB(30, 100, 200)
        closeTargetList()
    end
    print("–ú–µ–Ω—é: " .. (MainFrame.Visible and "–û—Ç–∫—Ä—ã—Ç–æ" or "–ó–∞–∫—Ä—ã—Ç–æ"))
end

openButton.MouseButton1Click:Connect(toggleMenu)
CloseButton.MouseButton1Click:Connect(toggleMenu)

UserInputService.InputBegan:Connect(function(input)
    if input.KeyCode == Enum.KeyCode.RightShift or input.KeyCode == Enum.KeyCode.Insert then
        toggleMenu()
    end
end)

-- ========== –û–ë–©–ò–ï –§–£–ù–ö–¶–ò–ò ==========
local function isEnemy(otherPlayer)
    if otherPlayer == player then return false end
    if player.Team and otherPlayer.Team then
        return player.Team ~= otherPlayer.Team
    end
    return true -- –ï—Å–ª–∏ –Ω–µ—Ç –∫–æ–º–∞–Ω–¥, —Å—á–∏—Ç–∞–µ–º –≤—Å–µ—Ö –≤—Ä–∞–≥–∞–º–∏
end

local function findClosestEnemy()
    local character = player.Character
    if not character then return nil end
    
    local rootPart = character:FindFirstChild("HumanoidRootPart")
    if not rootPart then return nil end
    
    local myPos = rootPart.Position
    local closest = nil
    local closestDist = math.huge
    
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if isEnemy(otherPlayer) and otherPlayer.Character then
            local enemyRoot = otherPlayer.Character:FindFirstChild("HumanoidRootPart")
            local enemyHumanoid = otherPlayer.Character:FindFirstChildOfClass("Humanoid")
            
            if enemyRoot and enemyHumanoid and enemyHumanoid.Health > 0 then
                local dist = (myPos - enemyRoot.Position).Magnitude
                if dist < closestDist and dist <= autoAimDistance then -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ
                    closestDist = dist
                    closest = otherPlayer
                end
            end
        end
    end
    
    return closest
end

-- ========== –ù–ê–°–¢–†–û–ï–ù–ù–´–ô –¢–†–ê–°–°–ï–† –ê–í–¢–û–ê–ò–ú–ê ==========
local function updateAutoAimTracer()
    if not settings.autoaim then
        if autoAimLine then
            autoAimLine:Destroy()
            autoAimLine = nil
        end
        return
    end

    local character = player.Character
    if not character then return end

    local enemy = findClosestEnemy()
    if not enemy or not enemy.Character then
        if autoAimLine then
            autoAimLine:Destroy()
            autoAimLine = nil
        end
        return
    end

    local myRoot = character:FindFirstChild("HumanoidRootPart")
    local enemyRoot = enemy.Character:FindFirstChild("HumanoidRootPart")
    if not myRoot or not enemyRoot then return end

    -- —Å–æ–∑–¥–∞—ë–º Part-—Ç—Ä–∞—Å—Å–µ—Ä, –µ—Å–ª–∏ –Ω–µ—Ç
    if not autoAimLine then
        autoAimLine = Instance.new("Part")
        autoAimLine.Name = "AutoAimTracer"
        autoAimLine.Anchored = true
        autoAimLine.CanCollide = false
        autoAimLine.Material = Enum.Material.Neon
        autoAimLine.Color = Color3.fromRGB(255, 50, 50)
        autoAimLine.Size = Vector3.new(0.5, 0.5, 0.5)
        autoAimLine.Transparency = 0.4
        autoAimLine.Parent = Workspace
    end

    -- –¥–∏—Å—Ç–∞–Ω—Ü–∏—è –¥–æ —Ü–µ–ª–∏
    local dist = (myRoot.Position - enemyRoot.Position).Magnitude

    -- —Ä–∞—Å—á—ë—Ç CFrame —Ç—Ä–∞—Å—Å—ã
    local beamCF = CFrame.new(myRoot.Position, enemyRoot.Position)
    local beamSize = Vector3.new(0.2, 0.2, dist)

    -- –ü–û–ú–ï–®–ê–¢–¨ –°–¢–û–õ–¨–ö–û, –ß–¢–û–ë–´ –õ–ò–ù–ò–Ø –ë–´–õ–ê –ñ–ò–†–ù–ï–ï
    autoAimLine.Size = Vector3.new(0.15, 0.15, dist) -- —Ä–∞–∑–º–µ—Ä –ª–∏–Ω–∏–∏
    autoAimLine.CFrame = beamCF * CFrame.new(0, 0, -dist/2)
    autoAimLine.Color = Color3.fromRGB(180, 0, 255) -- —è—Ä–∫–∏–π —Ü–≤–µ—Ç

end

-- ========== –§–£–ù–ö–¶–ò–Ø –û–ü–†–ï–î–ï–õ–ï–ù–ò–Ø –†–ï–ñ–ò–ú–ê –ö–ê–ú–ï–†–´ ==========
local function getCameraMode()
    local camera = Workspace.CurrentCamera
    if not camera then return "thirdperson" end
    
    -- –ü—Ä–æ–≤–µ—Ä—è–µ–º, –∫ —á–µ–º—É –ø—Ä–∏–≤—è–∑–∞–Ω–∞ –∫–∞–º–µ—Ä–∞
    local cameraSubject = camera.CameraSubject
    
    if cameraSubject and cameraSubject:IsA("Humanoid") then
        -- –ü—Ä–æ–≤–µ—Ä—è–µ–º —Ä–∞—Å—Å—Ç–æ—è–Ω–∏–µ –¥–æ –ø–µ—Ä—Å–æ–Ω–∞–∂–∞
        local character = cameraSubject.Parent
        if character then
            local distance = (camera.CFrame.Position - character:GetPivot().Position).Magnitude
            
            if distance < 5 then
                return "firstperson"
            else
                return "thirdperson"
            end
        end
    end
    
    return "thirdperson"
end

-- ========== AUTO AIM –° –†–ï–ñ–ò–ú–ê–ú–ò (–£–°–ö–û–†–ï–ù–ù–´–ô) ==========
local function startAutoAim()
    if connections.autoaim then 
        connections.autoaim:Disconnect()
        connections.autoaim = nil
    end
    
    print("[Auto Aim] ‚úÖ –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω (–†–µ–∂–∏–º: " .. autoAimMode .. ", –î–∏—Å—Ç–∞–Ω—Ü–∏—è: " .. autoAimDistance .. " studs)")
    
    connections.autoaim = RunService.Heartbeat:Connect(function()
        if not settings.autoaim then return end
        
        local character = player.Character
        if not character then return end
        
        local myRoot = character:FindFirstChild("HumanoidRootPart")
        if not myRoot then return end
        
        local enemy = findClosestEnemy()
        if not enemy or not enemy.Character then 
            return 
        end
        
        local enemyRoot = enemy.Character:FindFirstChild("HumanoidRootPart")
        if not enemyRoot then 
            return 
        end
        
        local distance = (myRoot.Position - enemyRoot.Position).Magnitude
        
        -- –û–±–Ω–æ–≤–ª—è–µ–º —Ç—Ä–∞—Å–µ—Ä (–≤–∏–¥–µ–Ω —á–µ—Ä–µ–∑ —Å—Ç–µ–Ω—ã)
        updateAutoAimTracer()
        
        if distance <= autoAimDistance then
            local direction = (enemyRoot.Position - myRoot.Position)
            
            -- –û–ø—Ä–µ–¥–µ–ª—è–µ–º —Ç–µ–∫—É—â–∏–π —Ä–µ–∂–∏–º
            local currentMode = autoAimMode
            if autoAimMode == "auto" then
                currentMode = getCameraMode()
            end
            
            -- –†–µ–∂–∏–º –æ—Ç –ø–µ—Ä–≤–æ–≥–æ –ª–∏—Ü–∞ (–£–°–ö–û–†–ï–ù–ù–´–ô)
            if currentMode == "firstperson" then
                local camera = Workspace.CurrentCamera
                if camera then
                    -- –ë–´–°–¢–†–´–ô –ø–æ–≤–æ—Ä–æ—Ç –∫–∞–º–µ—Ä—ã –∫ —Ü–µ–ª–∏ (–±–µ–∑ –ø–ª–∞–≤–Ω–æ—Å—Ç–∏)
                    camera.CFrame = CFrame.new(camera.CFrame.Position, enemyRoot.Position)
                end
            -- –†–µ–∂–∏–º –æ—Ç —Ç—Ä–µ—Ç—å–µ–≥–æ –ª–∏—Ü–∞ (–£–°–ö–û–†–ï–ù–ù–´–ô)
            else
                -- –ë—ã—Å—Ç—Ä—ã–π –≥–æ—Ä–∏–∑–æ–Ω—Ç–∞–ª—å–Ω—ã–π –ø–æ–≤–æ—Ä–æ—Ç –≥—É–º–∞–Ω–æ–∏–¥–∞
                direction = Vector3.new(direction.X, 0, direction.Z)
                if direction.Magnitude > 0 then
                    direction = direction.Unit
                    local lookPoint = myRoot.Position + direction
                    myRoot.CFrame = CFrame.new(myRoot.Position, Vector3.new(lookPoint.X, myRoot.Position.Y, lookPoint.Z))
                end
            end
        end
    end)
end

local function stopAutoAim()
    if connections.autoaim then
        connections.autoaim:Disconnect()
        connections.autoaim = nil
    end
    
    if autoAimLine then
        autoAimLine:Destroy()
        autoAimLine = nil
    end
    
    print("[Auto Aim] ‚ùå –í—ã–∫–ª—é—á–µ–Ω")
end

-- ========== –¢–ï–õ–ï–ü–û–†–¢ ==========
local function teleportForward()
    local now = tick()
    
    if now - lastTeleportTime < teleportCooldown then
        return
    end
    
    lastTeleportTime = now
    
    local character = player.Character
    if not character then
        print("[Teleport] –ù–µ—Ç –ø–µ—Ä—Å–æ–Ω–∞–∂–∞")
        return
    end
    
    local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
    if not humanoidRootPart then
        print("[Teleport] –ù–µ—Ç HumanoidRootPart")
        return
    end
    
    -- –ü–æ–ª—É—á–∞–µ–º –Ω–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –≤–∑–≥–ª—è–¥–∞ –∫–∞–º–µ—Ä—ã
    local camera = Workspace.CurrentCamera
    local lookVector = camera.CFrame.LookVector
    
    -- –£–±–∏—Ä–∞–µ–º –≤–µ—Ä—Ç–∏–∫–∞–ª—å–Ω—É—é —Å–æ—Å—Ç–∞–≤–ª—è—é—â—É—é –¥–ª—è –¥–≤–∏–∂–µ–Ω–∏—è —Ç–æ–ª—å–∫–æ –≤–ø–µ—Ä–µ–¥/–Ω–∞–∑–∞–¥
    local horizontalLookVector = Vector3.new(lookVector.X, 0, lookVector.Z)
    if horizontalLookVector.Magnitude > 0 then
        horizontalLookVector = horizontalLookVector.Unit
    else
        horizontalLookVector = Vector3.new(0, 0, 1) -- –ù–∞–ø—Ä–∞–≤–ª–µ–Ω–∏–µ –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
    end
    
    -- –ú–ì–ù–û–í–ï–ù–ù–´–ô —Ç–µ–ª–µ–ø–æ—Ä—Ç –≤–ø–µ—Ä–µ–¥ —á–µ—Ä–µ–∑ —Å—Ç–µ–Ω—ã
    local newPosition = humanoidRootPart.Position + (horizontalLookVector * teleportDistance)
    
    -- –í—Ä–µ–º–µ–Ω–Ω–æ –æ—Ç–∫–ª—é—á–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞
    local originalCollision = {}
    for _, part in pairs(character:GetDescendants()) do
        if part:IsA("BasePart") then
            originalCollision[part] = part.CanCollide
            part.CanCollide = false
        end
    end
    
    -- –ú–≥–Ω–æ–≤–µ–Ω–Ω–æ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä—É–µ–º
    humanoidRootPart.CFrame = CFrame.new(newPosition)
    
    -- –ñ–¥–µ–º –Ω–µ–º–Ω–æ–≥–æ –∏ –≤–æ—Å—Å—Ç–∞–Ω–∞–≤–ª–∏–≤–∞–µ–º –∫–æ–ª–ª–∏–∑–∏—é
    wait(0.05)
    for part, canCollide in pairs(originalCollision) do
        if part and part.Parent then
            part.CanCollide = canCollide
        end
    end
    
    -- –í—ã–≤–æ–¥–∏–º —Å–æ–æ–±—â–µ–Ω–∏–µ
    print("[Teleport] –ú–≥–Ω–æ–≤–µ–Ω–Ω–æ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω –Ω–∞ " .. teleportDistance .. " studs –≤–ø–µ—Ä–µ–¥")
    
    -- –û–±–Ω–æ–≤–ª—è–µ–º —Å—Ç–∞—Ç—É—Å –≤ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–µ
    if buttons.teleport and buttons.teleport.frame then
        local statusLabel = buttons.teleport.teleportStatusLabel
        if statusLabel then
            statusLabel.Text = "–¢–µ–ª–µ–ø–æ—Ä—Ç–∏—Ä–æ–≤–∞–Ω!"
            statusLabel.TextColor3 = Color3.fromRGB(0, 255, 200)
            
            delay(1, function()
                if statusLabel and settings.teleport then
                    statusLabel.Text = "–°—Ç–∞—Ç—É—Å: –í–∫–ª—é—á–µ–Ω–æ (Q)"
                    statusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
                end
            end)
        end
    end
end

-- ========== –£–õ–£–ß–®–ï–ù–ù–ê–Ø –≠–ö–†–ê–ù–ù–ê–Ø –ö–ù–û–ü–ö–ê –¢–ï–õ–ï–ü–û–†–¢–ê ==========
local function createTeleportScreenButton()
    if teleportScreenButton then
        teleportScreenButton:Destroy()
    end
    
    teleportScreenButton = Instance.new("TextButton")
    teleportScreenButton.Name = "TeleportScreenButton"
    teleportScreenButton.Size = UDim2.new(0, 60, 0, 60) -- –£–º–µ–Ω—å—à–∏–ª —Ä–∞–∑–º–µ—Ä
    teleportScreenButton.Position = UDim2.new(1, -70, 1, -70)
    teleportScreenButton.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
    teleportScreenButton.Text = "üöÄ"
    teleportScreenButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    teleportScreenButton.Font = Enum.Font.GothamBold
    teleportScreenButton.TextSize = 24
    teleportScreenButton.Visible = true
    teleportScreenButton.ZIndex = 9998
    teleportScreenButton.Parent = screenGui
    
    local buttonCorner = Instance.new("UICorner")
    buttonCorner.CornerRadius = UDim.new(0, 15)
    buttonCorner.Parent = teleportScreenButton
    
    local buttonStroke = Instance.new("UIStroke")
    buttonStroke.Color = Color3.fromRGB(255, 255, 255)
    buttonStroke.Thickness = 2
    buttonStroke.Parent = teleportScreenButton
    
    -- –ò–°–ü–†–ê–í–õ–ï–ù–ò–ï: –†–∞–∑–¥–µ–ª—è–µ–º –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏–µ –∏ –Ω–∞–∂–∞—Ç–∏–µ
    local dragging = false
    local dragInput, dragStart, startPos
    
    teleportScreenButton.InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseButton1 then
            -- –ï—Å–ª–∏ –∫–ª–∏–∫ –∫–æ—Ä–æ—Ç–∫–∏–π - —ç—Ç–æ —Ç–µ–ª–µ–ø–æ—Ä—Ç
            if not dragging then
                local clickTime = tick()
                local startPos = input.Position
                
                input.Changed:Connect(function()
                    if input.UserInputState == Enum.UserInputState.End then
                        local endPos = input.Position
                        local moveDistance = (endPos - startPos).Magnitude
                        local clickDuration = tick() - clickTime
                        
                        -- –ï—Å–ª–∏ –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏–µ –º–∞–ª–µ–Ω—å–∫–æ–µ –∏ –≤—Ä–µ–º—è –∫–æ—Ä–æ—Ç–∫–æ–µ - —ç—Ç–æ –∫–ª–∏–∫ –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞
                        if moveDistance < 10 and clickDuration < 0.3 then
                            teleportForward()
                        end
                        
                        dragging = false
                        teleportScreenButton.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
                    end
                end)
            end
        end
    end)
    
    -- –û—Ç–¥–µ–ª—å–Ω—ã–π –æ–±—Ä–∞–±–æ—Ç—á–∏–∫ –¥–ª—è –Ω–∞—á–∞–ª–∞ –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏—è
    teleportScreenButton.InputChanged:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.Touch or input.UserInputType == Enum.UserInputType.MouseMovement then
            dragInput = input
        end
    end)
    
    UserInputService.InputChanged:Connect(function(input)
        if input == dragInput and dragInput.UserInputState == Enum.UserInputState.Change then
            if not dragging then
                dragging = true
                dragStart = input.Position
                startPos = teleportScreenButton.Position
                teleportScreenButton.BackgroundColor3 = Color3.fromRGB(0, 200, 255)
            else
                local delta = input.Position - dragStart
                local newPosition = UDim2.new(
                    startPos.X.Scale, 
                    startPos.X.Offset + delta.X,
                    startPos.Y.Scale, 
                    startPos.Y.Offset + delta.Y
                )
                
                -- –û–≥—Ä–∞–Ω–∏—á–∏–≤–∞–µ–º –ø–µ—Ä–µ–º–µ—â–µ–Ω–∏–µ –≤ –ø—Ä–µ–¥–µ–ª–∞—Ö —ç–∫—Ä–∞–Ω–∞
                local viewportSize = screenGui.AbsoluteSize
                local buttonSize = teleportScreenButton.AbsoluteSize
                
                local minX = 0
                local maxX = viewportSize.X - buttonSize.X
                local minY = 0
                local maxY = viewportSize.Y - buttonSize.Y
                
                local absoluteX = startPos.X.Offset + delta.X
                local absoluteY = startPos.Y.Offset + delta.Y
                
                absoluteX = math.clamp(absoluteX, minX, maxX)
                absoluteY = math.clamp(absoluteY, minY, maxY)
                
                teleportScreenButton.Position = UDim2.new(0, absoluteX, 0, absoluteY)
            end
        end
    end)
    
    -- –°–±—Ä–∞—Å—ã–≤–∞–µ–º —Å–æ—Å—Ç–æ—è–Ω–∏–µ –ø–µ—Ä–µ—Ç–∞—Å–∫–∏–≤–∞–Ω–∏—è –ø—Ä–∏ –æ—Ç–ø—É—Å–∫–∞–Ω–∏–∏
    UserInputService.InputEnded:Connect(function(input)
        if input == dragInput then
            dragging = false
            teleportScreenButton.BackgroundColor3 = Color3.fromRGB(0, 150, 255)
        end
    end)
    
    print("[Teleport] –£–ª—É—á—à–µ–Ω–Ω–∞—è —ç–∫—Ä–∞–Ω–Ω–∞—è –∫–Ω–æ–ø–∫–∞ —Å–æ–∑–¥–∞–Ω–∞")
end

-- ========== –§–£–ù–ö–¶–ò–Ø –ê–ö–¢–ò–í–ê–¶–ò–ò –¢–ï–õ–ï–ü–û–†–¢–ê ==========
local function startTeleport()
    print("[Teleport] ‚úÖ –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω - –Ω–∞–∂–º–∏—Ç–µ Q –¥–ª—è –º–≥–Ω–æ–≤–µ–Ω–Ω–æ–≥–æ —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞ –≤–ø–µ—Ä–µ–¥")
    
    -- –û–±–Ω–æ–≤–ª—è–µ–º —Å—Ç–∞—Ç—É—Å –≤ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–µ
    if buttons.teleport and buttons.teleport.frame then
        local statusLabel = buttons.teleport.teleportStatusLabel
        if statusLabel then
            statusLabel.Text = "–°—Ç–∞—Ç—É—Å: –í–∫–ª—é—á–µ–Ω–æ (Q)"
            statusLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        end
    end
    
    -- –°–æ–∑–¥–∞–µ–º —ç–∫—Ä–∞–Ω–Ω—É—é –∫–Ω–æ–ø–∫—É –¥–ª—è –º–æ–±–∏–ª—å–Ω—ã—Ö —É—Å—Ç—Ä–æ–π—Å—Ç–≤
    createTeleportScreenButton()
    
    -- –ì–æ—Ä—è—á–∞—è –∫–ª–∞–≤–∏—à–∞ Q –¥–ª—è —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞
    connections.teleportKey = UserInputService.InputBegan:Connect(function(input)
        if input.KeyCode == Enum.KeyCode.Q and settings.teleport then
            teleportForward()
        end
    end)
end

-- ========== –§–£–ù–ö–¶–ò–Ø –î–ï–ê–ö–¢–ò–í–ê–¶–ò–ò –¢–ï–õ–ï–ü–û–†–¢–ê ==========
local function stopTeleport()
    if connections.teleportKey then
        connections.teleportKey:Disconnect()
        connections.teleportKey = nil
    end
    
    -- –£–¥–∞–ª—è–µ–º —ç–∫—Ä–∞–Ω–Ω—É—é –∫–Ω–æ–ø–∫—É
    if teleportScreenButton then
        teleportScreenButton:Destroy()
        teleportScreenButton = nil
    end
    
    -- –û–±–Ω–æ–≤–ª—è–µ–º —Å—Ç–∞—Ç—É—Å –≤ –∏–Ω—Ç–µ—Ä—Ñ–µ–π—Å–µ
    if buttons.teleport and buttons.teleport.frame then
        local statusLabel = buttons.teleport.teleportStatusLabel
        if statusLabel then
            statusLabel.Text = "–°—Ç–∞—Ç—É—Å: –í—ã–∫–ª—é—á–µ–Ω–æ (Q)"
            statusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        end
    end
    
    print("[Teleport] ‚ùå –í—ã–∫–ª—é—á–µ–Ω")
end

-- ========== AUTO SHOOT ==========
local function findWeapon()
    local character = player.Character
    if not character then return nil end
    
    for _, tool in pairs(character:GetChildren()) do
        if tool:IsA("Tool") then
            local name = string.lower(tool.Name)
            if name:find("mp5") or name:find("–º—Ä5") or name:find("gun") or name:find("rifle") or name:find("pistol") then
                return tool
            end
        end
    end
    return nil
end

local function performShootAtEnemy()
    local now = tick()
    if now - lastShot < shootCooldown then return false end
    
    if not shootRemote then
        return false
    end
    
    local weapon = findWeapon()
    if not weapon then
        return false
    end
    
    local enemy = nil
    if settings.pursuit then
        enemy = getPursuitTarget()
    end
    
    if not enemy then
        enemy = findClosestEnemy()
    end
    
    if not enemy then
        return false
    end
    
    local character = player.Character
    if not character then return false end
    
    pcall(function()
        weapon:Activate()
        
        local viewportSize = Workspace.CurrentCamera.ViewportSize
        VirtualInputManager:SendMouseButtonEvent(
            viewportSize.X / 2,
            viewportSize.Y / 2,
            0,
            true,
            game,
            1
        )
        
        task.wait(0.02)
        
        VirtualInputManager:SendMouseButtonEvent(
            viewportSize.X / 2,
            viewportSize.Y / 2,
            0,
            false,
            game,
            1
        )
    end)
    
    lastShot = now
    return true
end

local function startAutoShoot()
    if connections.autoshoot then return end
    
    print("[Auto Shoot] ‚úÖ –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω")
    
    connections.autoshoot = RunService.Heartbeat:Connect(function()
        if not settings.autoshoot then return end
        performShootAtEnemy()
    end)
end

local function stopAutoShoot()
    if connections.autoshoot then
        connections.autoshoot:Disconnect()
        connections.autoshoot = nil
    end
    print("[Auto Shoot] ‚ùå –í—ã–∫–ª—é—á–µ–Ω")
end

-- ========== ESP ==========
local function updateESP()
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character and not espHighlights[otherPlayer] then
            local highlight = Instance.new("Highlight")
            highlight.Name = "ESP_" .. otherPlayer.Name
            highlight.FillColor = isEnemy(otherPlayer) and Color3.fromRGB(255, 50, 50) or Color3.fromRGB(50, 255, 50)
            highlight.FillTransparency = 0.7
            highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
            highlight.OutlineTransparency = 0
            highlight.Adornee = otherPlayer.Character
            highlight.Parent = Workspace
            
            espHighlights[otherPlayer] = highlight
        end
    end
    
    for playerObj, highlight in pairs(espHighlights) do
        if playerObj and playerObj.Character and highlight then
            highlight.Adornee = playerObj.Character
            highlight.FillColor = isEnemy(playerObj) and Color3.fromRGB(255, 50, 50) or Color3.fromRGB(50, 255, 50)
        elseif highlight then
            highlight:Destroy()
            espHighlights[playerObj] = nil
        end
    end
end

local function startESP()
    print("[ESP] ‚úÖ –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω (–æ–±–Ω–æ–≤–ª–µ–Ω–∏–µ –∫–∞–∂–¥—ã–µ 5 —Å–µ–∫—É–Ω–¥)")
    
    updateESP()
    
    if connections.esp then
        connections.esp:Disconnect()
    end
    
    local lastUpdate = tick()
    connections.esp = RunService.Heartbeat:Connect(function()
        if not settings.esp then return end
        
        if tick() - lastUpdate >= 5 then
            updateESP()
            lastUpdate = tick()
        end
    end)
    
    Players.PlayerAdded:Connect(function(newPlayer)
        if settings.esp then
            wait(1)
            updateESP()
        end
    end)
    
    Players.PlayerRemoving:Connect(function(leavingPlayer)
        if settings.esp and espHighlights[leavingPlayer] then
            espHighlights[leavingPlayer]:Destroy()
            espHighlights[leavingPlayer] = nil
        end
    end)
end

local function stopESP()
    if connections.esp then
        connections.esp:Disconnect()
        connections.esp = nil
    end
    
    for _, highlight in pairs(espHighlights) do
        if highlight then
            highlight:Destroy()
        end
    end
    espHighlights = {}
    print("[ESP] ‚ùå –í—ã–∫–ª—é—á–µ–Ω")
end

-- ========== NOCLIP ==========
local function startNoclip()
    if connections.noclip then return end
    
    print("[Noclip] ‚úÖ –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω")
    connections.noclip = RunService.Stepped:Connect(function()
        local character = player.Character
        if character and settings.noclip then
            for _, part in pairs(character:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end
    end)
end

local function stopNoclip()
    if connections.noclip then
        connections.noclip:Disconnect()
        connections.noclip = nil
    end
    print("[Noclip] ‚ùå –í—ã–∫–ª—é—á–µ–Ω")
end

-- ========== SPEED HACK (–û–ë–ù–û–í–õ–ï–ù–ù–´–ô) ==========
local function updateSpeed()
    local character = player.Character
    if character and settings.speed then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = speedHackValue
            humanoid.JumpPower = math.min(100, speedHackValue + 20) -- –ü—Ä—ã–∂–æ–∫ –ø—Ä–æ–ø–æ—Ä—Ü–∏–æ–Ω–∞–ª—å–Ω–æ —Å–∫–æ—Ä–æ—Å—Ç–∏
        end
    end
end

local function startSpeed()
    if connections.speed then return end
    
    print("[Speed Hack] ‚úÖ –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω (–°–∫–æ—Ä–æ—Å—Ç—å: " .. speedHackValue .. ")")
    
    connections.speed = RunService.Heartbeat:Connect(function()
        if settings.speed then
            updateSpeed()
        end
    end)
    
    updateSpeed()
end

local function stopSpeed()
    if connections.speed then
        connections.speed:Disconnect()
        connections.speed = nil
    end
    
    local character = player.Character
    if character then
        local humanoid = character:FindFirstChildOfClass("Humanoid")
        if humanoid then
            humanoid.WalkSpeed = 16
            humanoid.JumpPower = 50
        end
    end
    print("[Speed Hack] ‚ùå –í—ã–∫–ª—é—á–µ–Ω")
end

-- ========== FULLBRIGHT ==========
local function startFullbright()
    print("[Fullbright] ‚úÖ –ê–∫—Ç–∏–≤–∏—Ä–æ–≤–∞–Ω (–í–µ—á–Ω—ã–π –¥–µ–Ω—å)")
    
    originalClockTime = Lighting.ClockTime
    
    Lighting.ClockTime = 14
    Lighting.Brightness = 2
    Lighting.OutdoorAmbient = Color3.fromRGB(200, 200, 200)
    Lighting.Ambient = Color3.fromRGB(150, 150, 150)
    Lighting.GlobalShadows = false
    
    if connections.fullbright then
        connections.fullbright:Disconnect()
    end
    
    connections.fullbright = RunService.Heartbeat:Connect(function()
        if settings.fullbright then
            Lighting.ClockTime = 14
            Lighting.Brightness = 2
        end
    end)
end

local function stopFullbright()
    if connections.fullbright then
        connections.fullbright:Disconnect()
        connections.fullbright = nil
    end
    
    Lighting.ClockTime = originalClockTime
    Lighting.Brightness = 1
    Lighting.OutdoorAmbient = Color3.fromRGB(128, 128, 128)
    Lighting.Ambient = Color3.fromRGB(128, 128, 128)
    Lighting.GlobalShadows = true
    
    print("[Fullbright] ‚ùå –í—ã–∫–ª—é—á–µ–Ω")
end

