local teleportScreenButton = nil

-- ========== –§–£–ù–ö–¶–ò–Ø –°–û–ó–î–ê–ù–ò–Ø –ö–ù–û–ü–ö–ò ==========
local function createCheatButton(name, description, id, showAdjustment, extraHeight, showModeSelection, showSpeedAdjustment, showTeleportAdjustment)
    local ButtonFrame = Instance.new("Frame")
    local totalHeight = (showAdjustment and 110 or 80) + (extraHeight or 0) + (showModeSelection and 50 or 0) + (showSpeedAdjustment and 40 or 0) + (showTeleportAdjustment and 40 or 0)
    ButtonFrame.Name = name .. "Frame"
    ButtonFrame.Size = UDim2.new(1, 0, 0, totalHeight)
    ButtonFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 50)
    ButtonFrame.Parent = ButtonContainer
    
    local ButtonCorner = Instance.new("UICorner")
    ButtonCorner.CornerRadius = UDim.new(0, 8)
    ButtonCorner.Parent = ButtonFrame
    
    -- –ò–∫–æ–Ω–∫–∞
    local Icon = Instance.new("TextLabel")
    Icon.Name = "Icon"
    Icon.Size = UDim2.new(0, 40, 0, 40)
    Icon.Position = UDim2.new(0, 10, 0, 15)
    Icon.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
    Icon.Text = "‚ö°"
    Icon.TextColor3 = Color3.fromRGB(255, 255, 255)
    Icon.Font = Enum.Font.GothamBold
    Icon.TextSize = 20
    Icon.Parent = ButtonFrame
    
    local IconCorner = Instance.new("UICorner")
    IconCorner.CornerRadius = UDim.new(0, 6)
    IconCorner.Parent = Icon
    
    -- –ù–∞–∑–≤–∞–Ω–∏–µ
    local ButtonName = Instance.new("TextLabel")
    ButtonName.Name = "ButtonName"
    ButtonName.Size = UDim2.new(0.6, 0, 0, 30)
    ButtonName.Position = UDim2.new(0, 60, 0, 15)
    ButtonName.BackgroundTransparency = 1
    ButtonName.Text = name
    ButtonName.TextColor3 = Color3.fromRGB(255, 255, 255)
    ButtonName.Font = Enum.Font.GothamBold
    ButtonName.TextSize = 16
    ButtonName.TextXAlignment = Enum.TextXAlignment.Left
    ButtonName.Parent = ButtonFrame
    
    -- –û–ø–∏—Å–∞–Ω–∏–µ
    local ButtonDesc = Instance.new("TextLabel")
    ButtonDesc.Name = "ButtonDesc"
    ButtonDesc.Size = UDim2.new(0.6, 0, 0, 20)
    ButtonDesc.Position = UDim2.new(0, 60, 0, 45)
    ButtonDesc.BackgroundTransparency = 1
    ButtonDesc.Text = description
    ButtonDesc.TextColor3 = Color3.fromRGB(180, 180, 200)
    ButtonDesc.Font = Enum.Font.Gotham
    ButtonDesc.TextSize = 12
    ButtonDesc.TextXAlignment = Enum.TextXAlignment.Left
    ButtonDesc.Parent = ButtonFrame
    
    -- –ö–Ω–æ–ø–∫–∞ –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏—è
    local ToggleButton = Instance.new("TextButton")
    ToggleButton.Name = "ToggleButton"
    ToggleButton.Size = UDim2.new(0, 80, 0, 35)
    ToggleButton.Position = UDim2.new(1, -90, 0, showAdjustment and 25 or 22)
    ToggleButton.BackgroundColor3 = Color3.fromRGB(80, 80, 100)
    ToggleButton.Text = "OFF"
    ToggleButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    ToggleButton.Font = Enum.Font.GothamBold
    ToggleButton.TextSize = 14
    ToggleButton.Parent = ButtonFrame
    
    local ToggleCorner = Instance.new("UICorner")
    ToggleCorner.CornerRadius = UDim.new(0, 6)
    ToggleCorner.Parent = ToggleButton
    
    -- –ö–Ω–æ–ø–∫–∏ —Ä–µ–≥—É–ª–∏—Ä–æ–≤–∫–∏ –¥–ª—è –∞–≤—Ç–æ–∞–∏–º–∞
    if showAdjustment then
        local AdjustmentFrame = Instance.new("Frame")
        AdjustmentFrame.Name = "AdjustmentFrame"
        AdjustmentFrame.Size = UDim2.new(1, -20, 0, 30)
        AdjustmentFrame.Position = UDim2.new(0, 10, 0, 80)
        AdjustmentFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 60)
        AdjustmentFrame.BorderSizePixel = 0
        AdjustmentFrame.Parent = ButtonFrame
        
        local AdjustmentCorner = Instance.new("UICorner")
        AdjustmentCorner.CornerRadius = UDim.new(0, 4)
        AdjustmentCorner.Parent = AdjustmentFrame
        
        -- –ö–Ω–æ–ø–∫–∞ "-" (—É–º–µ–Ω—å—à–∏—Ç—å –Ω–∞ 10)
        local MinusButton = Instance.new("TextButton")
        MinusButton.Name = "MinusButton"
        MinusButton.Size = UDim2.new(0, 40, 1, 0)
        MinusButton.Position = UDim2.new(0, 5, 0, 0)
        MinusButton.BackgroundColor3 = Color3.fromRGB(200, 60, 60)
        MinusButton.Text = "-10"
        MinusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        MinusButton.Font = Enum.Font.GothamBold
        MinusButton.TextSize = 14
        MinusButton.Parent = AdjustmentFrame
        
        local MinusCorner = Instance.new("UICorner")
        MinusCorner.CornerRadius = UDim.new(0, 4)
        MinusCorner.Parent = MinusButton
        
        -- –û—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ —Ç–µ–∫—É—â–µ–≥–æ –∑–Ω–∞—á–µ–Ω–∏—è
        local ValueLabel = Instance.new("TextLabel")
        ValueLabel.Name = "ValueLabel"
        ValueLabel.Size = UDim2.new(0.5, 0, 1, 0)
        ValueLabel.Position = UDim2.new(0.25, 0, 0, 0)
        ValueLabel.BackgroundTransparency = 1
        ValueLabel.Text = autoAimDistance .. " studs"
        ValueLabel.TextColor3 = Color3.fromRGB(0, 200, 255)
        ValueLabel.Font = Enum.Font.GothamBold
        ValueLabel.TextSize = 14
        ValueLabel.Parent = AdjustmentFrame
        
        -- –ö–Ω–æ–ø–∫–∞ "+" (—É–≤–µ–ª–∏—á–∏—Ç—å –Ω–∞ 10)
        local PlusButton = Instance.new("TextButton")
        PlusButton.Name = "PlusButton"
        PlusButton.Size = UDim2.new(0, 40, 1, 0)
        PlusButton.Position = UDim2.new(1, -45, 0, 0)
        PlusButton.BackgroundColor3 = Color3.fromRGB(60, 200, 60)
        PlusButton.Text = "+10"
        PlusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        PlusButton.Font = Enum.Font.GothamBold
        PlusButton.TextSize = 14
        PlusButton.Parent = AdjustmentFrame
        
        local PlusCorner = Instance.new("UICorner")
        PlusCorner.CornerRadius = UDim.new(0, 4)
        PlusCorner.Parent = PlusButton
        
        -- –ï—Å–ª–∏ –Ω—É–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –≤—ã–±–æ—Ä —Ä–µ–∂–∏–º–∞
        if showModeSelection then
            local ModeFrame = Instance.new("Frame")
            ModeFrame.Name = "ModeFrame"
            ModeFrame.Size = UDim2.new(1, -20, 0, 40)
            ModeFrame.Position = UDim2.new(0, 10, 0, 115)
            ModeFrame.BackgroundColor3 = Color3.fromRGB(40, 40, 55)
            ModeFrame.BorderSizePixel = 0
            ModeFrame.Parent = ButtonFrame
            
            local ModeCorner = Instance.new("UICorner")
            ModeCorner.CornerRadius = UDim.new(0, 4)
            ModeCorner.Parent = ModeFrame
            
            -- –¢–µ–∫—Å—Ç —Ä–µ–∂–∏–º–∞
            local ModeTextLabel = Instance.new("TextLabel")
            ModeTextLabel.Name = "ModeTextLabel"
            ModeTextLabel.Size = UDim2.new(0.4, 0, 0, 30)
            ModeTextLabel.Position = UDim2.new(0, 5, 0.5, -15)
            ModeTextLabel.BackgroundTransparency = 1
            ModeTextLabel.Text = "–†–µ–∂–∏–º:"
            ModeTextLabel.TextColor3 = Color3.fromRGB(200, 200, 255)
            ModeTextLabel.Font = Enum.Font.Gotham
            ModeTextLabel.TextSize = 12
            ModeTextLabel.TextXAlignment = Enum.TextXAlignment.Left
            ModeTextLabel.Parent = ModeFrame
            
            -- –ö–Ω–æ–ø–∫–∞ –ø–µ—Ä–µ–∫–ª—é—á–µ–Ω–∏—è —Ä–µ–∂–∏–º–∞
            local ModeButton = Instance.new("TextButton")
            ModeButton.Name = "ModeButton"
            ModeButton.Size = UDim2.new(0.55, 0, 0, 30)
            ModeButton.Position = UDim2.new(0.42, 0, 0.5, -15)
            ModeButton.BackgroundColor3 = Color3.fromRGB(60, 180, 60)
            ModeButton.Text = "AUTO"
            ModeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            ModeButton.Font = Enum.Font.GothamBold
            ModeButton.TextSize = 12
            ModeButton.Parent = ModeFrame
            
            local ModeButtonCorner = Instance.new("UICorner")
            ModeButtonCorner.CornerRadius = UDim.new(0, 4)
            ModeButtonCorner.Parent = ModeButton
            
            return {
                frame = ButtonFrame, 
                button = ToggleButton, 
                id = id,
                minusButton = MinusButton,
                plusButton = PlusButton,
                valueLabel = ValueLabel,
                modeButton = ModeButton
            }
        end
        
        return {
            frame = ButtonFrame, 
            button = ToggleButton, 
            id = id,
            minusButton = MinusButton,
            plusButton = PlusButton,
            valueLabel = ValueLabel
        }
    end
    
    -- –ï—Å–ª–∏ –Ω—É–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å —Ä–µ–≥—É–ª–∏—Ä–æ–≤–∫—É —Å–∫–æ—Ä–æ—Å—Ç–∏
    if showSpeedAdjustment then
        local SpeedFrame = Instance.new("Frame")
        SpeedFrame.Name = "SpeedFrame"
        SpeedFrame.Size = UDim2.new(1, -20, 0, 30)
        SpeedFrame.Position = UDim2.new(0, 10, 0, 80)
        SpeedFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 60)
        SpeedFrame.BorderSizePixel = 0
        SpeedFrame.Parent = ButtonFrame
        
        local SpeedCorner = Instance.new("UICorner")
        SpeedCorner.CornerRadius = UDim.new(0, 4)
        SpeedCorner.Parent = SpeedFrame
        
        -- –ö–Ω–æ–ø–∫–∞ "-" (—É–º–µ–Ω—å—à–∏—Ç—å –Ω–∞ 1)
        local SpeedMinusButton = Instance.new("TextButton")
        SpeedMinusButton.Name = "SpeedMinusButton"
        SpeedMinusButton.Size = UDim2.new(0, 40, 1, 0)
        SpeedMinusButton.Position = UDim2.new(0, 5, 0, 0)
        SpeedMinusButton.BackgroundColor3 = Color3.fromRGB(200, 60, 60)
        SpeedMinusButton.Text = "-1"
        SpeedMinusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        SpeedMinusButton.Font = Enum.Font.GothamBold
        SpeedMinusButton.TextSize = 14
        SpeedMinusButton.Parent = SpeedFrame
        
        local SpeedMinusCorner = Instance.new("UICorner")
        SpeedMinusCorner.CornerRadius = UDim.new(0, 4)
        SpeedMinusCorner.Parent = SpeedMinusButton
        
        -- –û—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ —Ç–µ–∫—É—â–µ–π —Å–∫–æ—Ä–æ—Å—Ç–∏
        local SpeedValueLabel = Instance.new("TextLabel")
        SpeedValueLabel.Name = "SpeedValueLabel"
        SpeedValueLabel.Size = UDim2.new(0.5, 0, 1, 0)
        SpeedValueLabel.Position = UDim2.new(0.25, 0, 0, 0)
        SpeedValueLabel.BackgroundTransparency = 1
        SpeedValueLabel.Text = speedHackValue .. " speed"
        SpeedValueLabel.TextColor3 = Color3.fromRGB(0, 200, 255)
        SpeedValueLabel.Font = Enum.Font.GothamBold
        SpeedValueLabel.TextSize = 14
        SpeedValueLabel.Parent = SpeedFrame
        
        -- –ö–Ω–æ–ø–∫–∞ "+" (—É–≤–µ–ª–∏—á–∏—Ç—å –Ω–∞ 1)
        local SpeedPlusButton = Instance.new("TextButton")
        SpeedPlusButton.Name = "SpeedPlusButton"
        SpeedPlusButton.Size = UDim2.new(0, 40, 1, 0)
        SpeedPlusButton.Position = UDim2.new(1, -45, 0, 0)
        SpeedPlusButton.BackgroundColor3 = Color3.fromRGB(60, 200, 60)
        SpeedPlusButton.Text = "+1"
        SpeedPlusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        SpeedPlusButton.Font = Enum.Font.GothamBold
        SpeedPlusButton.TextSize = 14
        SpeedPlusButton.Parent = SpeedFrame
        
        local SpeedPlusCorner = Instance.new("UICorner")
        SpeedPlusCorner.CornerRadius = UDim.new(0, 4)
        SpeedPlusCorner.Parent = SpeedPlusButton
        
        return {
            frame = ButtonFrame, 
            button = ToggleButton, 
            id = id,
            speedMinusButton = SpeedMinusButton,
            speedPlusButton = SpeedPlusButton,
            speedValueLabel = SpeedValueLabel
        }
    end
    
    -- –ï—Å–ª–∏ –Ω—É–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å —Ä–µ–≥—É–ª–∏—Ä–æ–≤–∫—É —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞
    if showTeleportAdjustment then
        local TeleportFrame = Instance.new("Frame")
        TeleportFrame.Name = "TeleportFrame"
        TeleportFrame.Size = UDim2.new(1, -20, 0, 30)
        TeleportFrame.Position = UDim2.new(0, 10, 0, 80)
        TeleportFrame.BackgroundColor3 = Color3.fromRGB(45, 45, 60)
        TeleportFrame.BorderSizePixel = 0
        TeleportFrame.Parent = ButtonFrame
        
        local TeleportCorner = Instance.new("UICorner")
        TeleportCorner.CornerRadius = UDim.new(0, 4)
        TeleportCorner.Parent = TeleportFrame
        
        -- –ö–Ω–æ–ø–∫–∞ "-" (—É–º–µ–Ω—å—à–∏—Ç—å –Ω–∞ 5)
        local TeleportMinusButton = Instance.new("TextButton")
        TeleportMinusButton.Name = "TeleportMinusButton"
        TeleportMinusButton.Size = UDim2.new(0, 40, 1, 0)
        TeleportMinusButton.Position = UDim2.new(0, 5, 0, 0)
        TeleportMinusButton.BackgroundColor3 = Color3.fromRGB(200, 60, 60)
        TeleportMinusButton.Text = "-5"
        TeleportMinusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TeleportMinusButton.Font = Enum.Font.GothamBold
        TeleportMinusButton.TextSize = 14
        TeleportMinusButton.Parent = TeleportFrame
        
        local TeleportMinusCorner = Instance.new("UICorner")
        TeleportMinusCorner.CornerRadius = UDim.new(0, 4)
        TeleportMinusCorner.Parent = TeleportMinusButton
        
        -- –û—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ —Ç–µ–∫—É—â–µ–π –¥–∏—Å—Ç–∞–Ω—Ü–∏–∏
        local TeleportValueLabel = Instance.new("TextLabel")
        TeleportValueLabel.Name = "TeleportValueLabel"
        TeleportValueLabel.Size = UDim2.new(0.5, 0, 1, 0)
        TeleportValueLabel.Position = UDim2.new(0.25, 0, 0, 0)
        TeleportValueLabel.BackgroundTransparency = 1
        TeleportValueLabel.Text = teleportDistance .. " studs"
        TeleportValueLabel.TextColor3 = Color3.fromRGB(0, 200, 255)
        TeleportValueLabel.Font = Enum.Font.GothamBold
        TeleportValueLabel.TextSize = 14
        TeleportValueLabel.Parent = TeleportFrame
        
        -- –ö–Ω–æ–ø–∫–∞ "+" (—É–≤–µ–ª–∏—á–∏—Ç—å –Ω–∞ 5)
        local TeleportPlusButton = Instance.new("TextButton")
        TeleportPlusButton.Name = "TeleportPlusButton"
        TeleportPlusButton.Size = UDim2.new(0, 40, 1, 0)
        TeleportPlusButton.Position = UDim2.new(1, -45, 0, 0)
        TeleportPlusButton.BackgroundColor3 = Color3.fromRGB(60, 200, 60)
        TeleportPlusButton.Text = "+5"
        TeleportPlusButton.TextColor3 = Color3.fromRGB(255, 255, 255)
        TeleportPlusButton.Font = Enum.Font.GothamBold
        TeleportPlusButton.TextSize = 14
        TeleportPlusButton.Parent = TeleportFrame
        
        local TeleportPlusCorner = Instance.new("UICorner")
        TeleportPlusCorner.CornerRadius = UDim.new(0, 4)
        TeleportPlusCorner.Parent = TeleportPlusButton
        
        -- –°—Ç–∞—Ç—É—Å —Ç–µ–ª–µ–ø–æ—Ä—Ç–∞
        local TeleportStatusLabel = Instance.new("TextLabel")
        TeleportStatusLabel.Name = "TeleportStatusLabel"
        TeleportStatusLabel.Size = UDim2.new(1, -20, 0, 18)
        TeleportStatusLabel.Position = UDim2.new(0, 10, 0, 115)
        TeleportStatusLabel.BackgroundTransparency = 1
        TeleportStatusLabel.Text = "–°—Ç–∞—Ç—É—Å: –í—ã–∫–ª—é—á–µ–Ω–æ (Q)"
        TeleportStatusLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        TeleportStatusLabel.Font = Enum.Font.Gotham
        TeleportStatusLabel.TextSize = 10
        TeleportStatusLabel.TextXAlignment = Enum.TextXAlignment.Left
        TeleportStatusLabel.Parent = ButtonFrame
        
        return {
            frame = ButtonFrame, 
            button = ToggleButton, 
            id = id,
            teleportMinusButton = TeleportMinusButton,
            teleportPlusButton = TeleportPlusButton,
            teleportValueLabel = TeleportValueLabel,
            teleportStatusLabel = TeleportStatusLabel
        }
    end
    
    return {frame = ButtonFrame, button = ToggleButton, id = id}
end

-- ========== –°–û–ó–î–ê–ï–ú –ö–ù–û–ü–ö–ò ==========
local buttons = {
    autoaim = createCheatButton("AUTO AIM", "–ü–æ–≤–æ—Ä–æ—Ç –∫ –±–ª–∏–∂–∞–π—à–µ–º—É –≤—Ä–∞–≥—É + —Ä–µ–∂–∏–º—ã", "autoaim", true, 0, true),
    autoshoot = createCheatButton("AUTO SHOOT", "–ê–≤—Ç–æ-—Å—Ç—Ä–µ–ª—å–±–∞ –¥–ª—è –ú–†5", "autoshoot"),
    esp = createCheatButton("ESP", "–ü–æ–¥—Å–≤–µ—Ç–∫–∞ –≤—Å–µ—Ö –∏–≥—Ä–æ–∫–æ–≤", "esp"),
    noclip = createCheatButton("NOCLIP", "–•–æ–¥–∏—Ç—å —Å–∫–≤–æ–∑—å —Å—Ç–µ–Ω—ã", "noclip"),
    speed = createCheatButton("SPEED HACK", "–†–µ–≥—É–ª–∏—Ä–æ–≤–∫–∞ —Å–∫–æ—Ä–æ—Å—Ç–∏", "speed", false, 0, false, true),
    fullbright = createCheatButton("FULLBRIGHT", "–í–µ—á–Ω—ã–π –¥–µ–Ω—å –∏ —è—Ä–∫–æ—Å—Ç—å", "fullbright"),
    nofog = createCheatButton("NO FOG", "–£–±—Ä–∞—Ç—å —Ç—É–º–∞–Ω –ø–æ–ª–Ω–æ—Å—Ç—å—é", "nofog"),
    zoom = createCheatButton("MAX ZOOM", "–£–≤–µ–ª–∏—á–∏—Ç—å –¥–∞–ª—å–Ω–æ—Å—Ç—å –æ–±–∑–æ—Ä–∞", "zoom"),
    pursuit = createCheatButton("PURSUIT", "–ü—Ä–µ—Å–ª–µ–¥–æ–≤–∞–Ω–∏–µ —Ü–µ–ª–∏", "pursuit", false, 80),
    teleport = createCheatButton("TELEPORT", "–ú–≥–Ω–æ–≤–µ–Ω–Ω—ã–π —Ç–µ–ª–µ–ø–æ—Ä—Ç –≤–ø–µ—Ä–µ–¥", "teleport", false, 0, false, false, true)
}

-- –ò–∫–æ–Ω–∫–∏ –¥–ª—è –∫–Ω–æ–ø–æ–∫
local icons = {
    autoaim = "üéØ",
    autoshoot = "üî´",
    esp = "üëÅÔ∏è",
    noclip = "üö∑",
    speed = "‚ö°",
    fullbright = "‚òÄÔ∏è",
    nofog = "üå´Ô∏è",
    zoom = "üîç",
    pursuit = "üéØüèÉ‚Äç‚ôÇÔ∏è",
    teleport = "üöÄ"
}

for id, buttonData in pairs(buttons) do
    if icons[id] then
        buttonData.frame.Icon.Text = icons[id]
    end
end

-- ========== –§–£–ù–ö–¶–ò–Ø –û–ë–ù–û–í–õ–ï–ù–ò–Ø –†–ï–ñ–ò–ú–û–í AUTO AIM ==========
local function updateAutoAimModeDisplay()
    if not buttons.autoaim or not buttons.autoaim.modeButton then return end
    
    local modeButton = buttons.autoaim.modeButton
    
    if autoAimMode == "auto" then
        modeButton.Text = "AUTO"
        modeButton.BackgroundColor3 = Color3.fromRGB(60, 180, 60)
    elseif autoAimMode == "firstperson" then
        modeButton.Text = "1st Person"
        modeButton.BackgroundColor3 = Color3.fromRGB(80, 120, 200)
    else
        modeButton.Text = "3rd Person"
        modeButton.BackgroundColor3 = Color3.fromRGB(200, 120, 80)
    end
    
    print("[Auto Aim] –†–µ–∂–∏–º –∏–∑–º–µ–Ω–µ–Ω –Ω–∞: " .. autoAimMode)
end

-- ========== –§–£–ù–ö–¶–ò–Ø –ü–ï–†–ï–ö–õ–Æ–ß–ï–ù–ò–Ø –†–ï–ñ–ò–ú–û–í AUTO AIM ==========
local function switchAutoAimMode()
    if autoAimMode == "auto" then
        autoAimMode = "firstperson"
    elseif autoAimMode == "firstperson" then
        autoAimMode = "thirdperson"
    else
        autoAimMode = "auto"
    end
    
    updateAutoAimModeDisplay()
    
    -- –ï—Å–ª–∏ Auto Aim –≤–∫–ª—é—á–µ–Ω, –ø–µ—Ä–µ–∑–∞–ø—É—Å–∫–∞–µ–º –µ–≥–æ
    if settings.autoaim then
        stopAutoAim()
        startAutoAim()
    end
end

-- ========== –§–£–ù–ö–¶–ò–Ø –û–ë–ù–û–í–õ–ï–ù–ò–Ø –î–ò–°–ü–õ–ï–Ø –ê–í–¢–û–ê–ò–ú–ê ==========
local function updateAutoAimDisplay()
    if buttons.autoaim and buttons.autoaim.valueLabel then
        buttons.autoaim.valueLabel.Text = autoAimDistance .. " studs"
        
        -- –†–∞–∑–Ω—ã–µ —Ü–≤–µ—Ç–∞ –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç —Ä–∞—Å—Å—Ç–æ—è–Ω–∏—è
        if autoAimDistance <= 50 then
            buttons.autoaim.valueLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        elseif autoAimDistance <= 150 then
            buttons.autoaim.valueLabel.TextColor3 = Color3.fromRGB(255, 200, 100)
        elseif autoAimDistance <= 300 then
            buttons.autoaim.valueLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        elseif autoAimDistance <= 400 then
            buttons.autoaim.valueLabel.TextColor3 = Color3.fromRGB(100, 200, 255)
        else
            buttons.autoaim.valueLabel.TextColor3 = Color3.fromRGB(200, 100, 255)
        end
    end
end

-- ========== –§–£–ù–ö–¶–ò–Ø –û–ë–ù–û–í–õ–ï–ù–ò–Ø –î–ò–°–ü–õ–ï–Ø SPEED HACK ==========
local function updateSpeedHackDisplay()
    if buttons.speed and buttons.speed.speedValueLabel then
        buttons.speed.speedValueLabel.Text = speedHackValue .. " speed"
        
        -- –†–∞–∑–Ω—ã–µ —Ü–≤–µ—Ç–∞ –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç —Å–∫–æ—Ä–æ—Å—Ç–∏
        if speedHackValue <= 16 then
            buttons.speed.speedValueLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        elseif speedHackValue <= 30 then
            buttons.speed.speedValueLabel.TextColor3 = Color3.fromRGB(255, 200, 100)
        elseif speedHackValue <= 50 then
            buttons.speed.speedValueLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        elseif speedHackValue <= 80 then
            buttons.speed.speedValueLabel.TextColor3 = Color3.fromRGB(100, 200, 255)
        else
            buttons.speed.speedValueLabel.TextColor3 = Color3.fromRGB(200, 100, 255)
        end
    end
end

-- ========== –§–£–ù–ö–¶–ò–Ø –û–ë–ù–û–í–õ–ï–ù–ò–Ø –î–ò–°–ü–õ–ï–Ø –¢–ï–õ–ï–ü–û–†–¢–ê ==========
local function updateTeleportDisplay()
    if buttons.teleport and buttons.teleport.teleportValueLabel then
        buttons.teleport.teleportValueLabel.Text = teleportDistance .. " studs"
        
        -- –†–∞–∑–Ω—ã–µ —Ü–≤–µ—Ç–∞ –≤ –∑–∞–≤–∏—Å–∏–º–æ—Å—Ç–∏ –æ—Ç –¥–∏—Å—Ç–∞–Ω—Ü–∏–∏
        if teleportDistance <= 10 then
            buttons.teleport.teleportValueLabel.TextColor3 = Color3.fromRGB(255, 100, 100)
        elseif teleportDistance <= 30 then
            buttons.teleport.teleportValueLabel.TextColor3 = Color3.fromRGB(255, 200, 100)
        elseif teleportDistance <= 60 then
            buttons.teleport.teleportValueLabel.TextColor3 = Color3.fromRGB(100, 255, 100)
        elseif teleportDistance <= 100 then
            buttons.teleport.teleportValueLabel.TextColor3 = Color3.fromRGB(100, 200, 255)
        else
            buttons.teleport.teleportValueLabel.TextColor3 = Color3.fromRGB(200, 100, 255)
        end
    end
end

-- ========== –û–ë–†–ê–ë–û–¢–ß–ò–ö–ò –ö–ù–û–ü–û–ö –†–ï–ì–£–õ–ò–†–û–í–ö–ò –ê–í–¢–û–ê–ò–ú–ê ==========
if buttons.autoaim and buttons.autoaim.minusButton then
    buttons.autoaim.minusButton.MouseButton1Click:Connect(function()
        autoAimDistance = math.max(10, autoAimDistance - 10) -- –ú–∏–Ω–∏–º—É–º 10, —à–∞–≥ 10
        updateAutoAimDisplay()
        print("[Auto Aim] –î–∏—Å—Ç–∞–Ω—Ü–∏—è —É–º–µ–Ω—å—à–µ–Ω–∞: " .. autoAimDistance .. " studs")
        
        local originalColor = buttons.autoaim.minusButton.BackgroundColor3
        buttons.autoaim.minusButton.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
        wait(0.1)
        buttons.autoaim.minusButton.BackgroundColor3 = originalColor
    end)
end

if buttons.autoaim and buttons.autoaim.plusButton then
    buttons.autoaim.plusButton.MouseButton1Click:Connect(function()
        autoAimDistance = math.min(500, autoAimDistance + 10) -- –ú–∞–∫—Å–∏–º—É–º 500, —à–∞–≥ 10
        updateAutoAimDisplay()
        print("[Auto Aim] –î–∏—Å—Ç–∞–Ω—Ü–∏—è —É–≤–µ–ª–∏—á–µ–Ω–∞: " .. autoAimDistance .. " studs")
        
        local originalColor = buttons.autoaim.plusButton.BackgroundColor3
        buttons.autoaim.plusButton.BackgroundColor3 = Color3.fromRGB(80, 255, 80)
        wait(0.1)
        buttons.autoaim.plusButton.BackgroundColor3 = originalColor
    end)
end

-- ========== –û–ë–†–ê–ë–û–¢–ß–ò–ö–ò –ö–ù–û–ü–û–ö –†–ï–ì–£–õ–ò–†–û–í–ö–ò SPEED HACK ==========
if buttons.speed and buttons.speed.speedMinusButton then
    buttons.speed.speedMinusButton.MouseButton1Click:Connect(function()
        speedHackValue = math.max(1, speedHackValue - 1) -- –ú–∏–Ω–∏–º—É–º 1, —à–∞–≥ 1
        updateSpeedHackDisplay()
        print("[Speed Hack] –°–∫–æ—Ä–æ—Å—Ç—å —É–º–µ–Ω—å—à–µ–Ω–∞: " .. speedHackValue)
        
        local originalColor = buttons.speed.speedMinusButton.BackgroundColor3
        buttons.speed.speedMinusButton.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
        wait(0.1)
        buttons.speed.speedMinusButton.BackgroundColor3 = originalColor
        
        -- –ï—Å–ª–∏ Speed Hack –≤–∫–ª—é—á–µ–Ω, –æ–±–Ω–æ–≤–ª—è–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å
        if settings.speed then
            updateSpeed()
        end
    end)
end

if buttons.speed and buttons.speed.speedPlusButton then
    buttons.speed.speedPlusButton.MouseButton1Click:Connect(function()
        speedHackValue = math.min(200, speedHackValue + 1) -- –ú–∞–∫—Å–∏–º—É–º 200, —à–∞–≥ 1
        updateSpeedHackDisplay()
        print("[Speed Hack] –°–∫–æ—Ä–æ—Å—Ç—å —É–≤–µ–ª–∏—á–µ–Ω–∞: " .. speedHackValue)
        
        local originalColor = buttons.speed.speedPlusButton.BackgroundColor3
        buttons.speed.speedPlusButton.BackgroundColor3 = Color3.fromRGB(80, 255, 80)
        wait(0.1)
        buttons.speed.speedPlusButton.BackgroundColor3 = originalColor
        
        -- –ï—Å–ª–∏ Speed Hack –≤–∫–ª—é—á–µ–Ω, –æ–±–Ω–æ–≤–ª—è–µ–º —Å–∫–æ—Ä–æ—Å—Ç—å
        if settings.speed then
            updateSpeed()
        end
    end)
end

-- ========== –û–ë–†–ê–ë–û–¢–ß–ò–ö–ò –ö–ù–û–ü–û–ö –†–ï–ì–£–õ–ò–†–û–í–ö–ò –¢–ï–õ–ï–ü–û–†–¢–ê ==========
if buttons.teleport and buttons.teleport.teleportMinusButton then
    buttons.teleport.teleportMinusButton.MouseButton1Click:Connect(function()
        teleportDistance = math.max(5, teleportDistance - 5) -- –ú–∏–Ω–∏–º—É–º 5, —à–∞–≥ 5
        updateTeleportDisplay()
        print("[Teleport] –î–∏—Å—Ç–∞–Ω—Ü–∏—è —É–º–µ–Ω—å—à–µ–Ω–∞: " .. teleportDistance .. " studs")
        
        local originalColor = buttons.teleport.teleportMinusButton.BackgroundColor3
        buttons.teleport.teleportMinusButton.BackgroundColor3 = Color3.fromRGB(255, 80, 80)
        wait(0.1)
        buttons.teleport.teleportMinusButton.BackgroundColor3 = originalColor
    end)
end

if buttons.teleport and buttons.teleport.teleportPlusButton then
    buttons.teleport.teleportPlusButton.MouseButton1Click:Connect(function()
        teleportDistance = math.min(200, teleportDistance + 5) -- –ú–∞–∫—Å–∏–º—É–º 200, —à–∞–≥ 5
        updateTeleportDisplay()
        print("[Teleport] –î–∏—Å—Ç–∞–Ω—Ü–∏—è —É–≤–µ–ª–∏—á–µ–Ω–∞: " .. teleportDistance .. " studs")
        
        local originalColor = buttons.teleport.teleportPlusButton.BackgroundColor3
        buttons.teleport.teleportPlusButton.BackgroundColor3 = Color3.fromRGB(80, 255, 80)
        wait(0.1)
        buttons.teleport.teleportPlusButton.BackgroundColor3 = originalColor
    end)
end

-- ========== –û–ë–†–ê–ë–û–¢–ß–ò–ö–ò –ö–ù–û–ü–û–ö –†–ï–ñ–ò–ú–û–í ==========
if buttons.autoaim and buttons.autoaim.modeButton then
    buttons.autoaim.modeButton.MouseButton1Click:Connect(switchAutoAimMode)
end

-- –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∏—Ä—É–µ–º –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ
updateAutoAimDisplay()
updateSpeedHackDisplay()
updateTeleportDisplay()
updateAutoAimModeDisplay()

-- ========== –ù–û–í–ê–Ø –°–ò–°–¢–ï–ú–ê –í–´–ë–û–†–ê –¶–ï–õ–ò (–ü–†–û–°–¢–ê–Ø –ò –†–ê–ë–û–ß–ê–Ø) ==========
-- –°–æ–∑–¥–∞–µ–º —ç–ª–µ–º–µ–Ω—Ç—ã –¥–ª—è –≤—ã–±–æ—Ä–∞ —Ü–µ–ª–∏
local targetSelectionFrame = nil
local isTargetListOpen = false

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è —Å–ø–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–æ–≤
local function updateTargetList()
    if not targetSelectionFrame then return end
    
    local scrollFrame = targetSelectionFrame:FindFirstChild("PlayersScroll")
    if not scrollFrame then return end
    
    -- –û—á–∏—â–∞–µ–º —Å—Ç–∞—Ä—ã–µ –∫–Ω–æ–ø–∫–∏
    for _, child in pairs(scrollFrame:GetChildren()) do
        if child:IsA("TextButton") then
            child:Destroy()
        end
    end
    
    local function isEnemy(otherPlayer)
        if otherPlayer == player then return false end
        if player.Team and otherPlayer.Team then
            return player.Team ~= otherPlayer.Team
        end
        return true
    end
    
    -- –°–æ–∑–¥–∞–µ–º –∫–Ω–æ–ø–∫–∏ –¥–ª—è –∫–∞–∂–¥–æ–≥–æ –∏–≥—Ä–æ–∫–∞
    for _, otherPlayer in pairs(Players:GetPlayers()) do
        if otherPlayer ~= player and otherPlayer.Character then
            local enemyRoot = otherPlayer.Character:FindFirstChild("HumanoidRootPart")
            local enemyHumanoid = otherPlayer.Character:FindFirstChildOfClass("Humanoid")
            
            if enemyRoot and enemyHumanoid and enemyHumanoid.Health > 0 then
                local playerButton = Instance.new("TextButton")
                playerButton.Name = otherPlayer.Name
                playerButton.Size = UDim2.new(1, -10, 0, 25)
                playerButton.Position = UDim2.new(0, 5, 0, 0)
                playerButton.BackgroundColor3 = isEnemy(otherPlayer) and Color3.fromRGB(180, 60, 60) or Color3.fromRGB(60, 120, 60)
                playerButton.Text = (isEnemy(otherPlayer) and "üî¥ " or "üü¢ ") .. otherPlayer.Name
                playerButton.TextColor3 = Color3.fromRGB(255, 255, 255)
                playerButton.Font = Enum.Font.Gotham
                playerButton.TextSize = 12
                playerButton.TextXAlignment = Enum.TextXAlignment.Left
                playerButton.Parent = scrollFrame
                
                local playerCorner = Instance.new("UICorner")
                playerCorner.CornerRadius = UDim.new(0, 4)
                playerCorner.Parent = playerButton
                
                playerButton.MouseButton1Click:Connect(function()
                    selectedTarget = otherPlayer
                    isAutoTarget = false
                    print("[Target] –í—ã–±—Ä–∞–Ω–∞ —Ü–µ–ª—å: " .. otherPlayer.Name)
                    
                    -- –û–±–Ω–æ–≤–ª—è–µ–º –æ—Ç–æ–±—Ä–∞–∂–µ–Ω–∏–µ –≤—ã–±—Ä–∞–Ω–Ω–æ–π —Ü–µ–ª–∏
                    if buttons.pursuit and buttons.pursuit.frame then
                        local targetLabel = buttons.pursuit.frame:FindFirstChild("TargetLabel")
                        if targetLabel then
                            targetLabel.Text = "–¶–µ–ª—å: " .. otherPlayer.Name .. (isEnemy(otherPlayer) and " (–≤—Ä–∞–≥)" or " (—Å–æ—é–∑–Ω–∏–∫)")
                        end
                    end
                    
                    -- –ó–∞–∫—Ä—ã–≤–∞–µ–º —Å–ø–∏—Å–æ–∫
                    closeTargetList()
                end)
            end
        end
    end
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –æ—Ç–∫—Ä—ã—Ç–∏—è —Å–ø–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–æ–≤
local function openTargetList()
    if isTargetListOpen or not buttons.pursuit then return end
    
    isTargetListOpen = true
    
    -- –°–æ–∑–¥–∞–µ–º —Ñ—Ä–µ–π–º –¥–ª—è —Å–ø–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–æ–≤
    targetSelectionFrame = Instance.new("Frame")
    targetSelectionFrame.Name = "TargetSelectionFrame"
    targetSelectionFrame.Size = UDim2.new(0, 280, 0, 220)
    targetSelectionFrame.Position = UDim2.new(0.5, -140, 0.5, -110)
    targetSelectionFrame.BackgroundColor3 = Color3.fromRGB(35, 35, 50)
    targetSelectionFrame.BorderSizePixel = 0
    targetSelectionFrame.ZIndex = 10000
    targetSelectionFrame.Parent = screenGui
    
    local listCorner = Instance.new("UICorner")
    listCorner.CornerRadius = UDim.new(0, 10)
    listCorner.Parent = targetSelectionFrame
    
    -- –ó–∞–≥–æ–ª–æ–≤–æ–∫
    local titleLabel = Instance.new("TextLabel")
    titleLabel.Name = "TitleLabel"
    titleLabel.Size = UDim2.new(1, 0, 0, 30)
    titleLabel.Position = UDim2.new(0, 0, 0, 0)
    titleLabel.BackgroundColor3 = Color3.fromRGB(50, 50, 70)
    titleLabel.Text = "üë• –í–´–ë–û–† –¶–ï–õ–ò –î–õ–Ø –ü–†–ï–°–õ–ï–î–û–í–ê–ù–ò–Ø"
    titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
    titleLabel.Font = Enum.Font.GothamBold
    titleLabel.TextSize = 14
    titleLabel.Parent = targetSelectionFrame
    
    local titleCorner = Instance.new("UICorner")
    titleCorner.CornerRadius = UDim.new(0, 10)
    titleCorner.Parent = titleLabel
    
    -- –ö–Ω–æ–ø–∫–∞ –∑–∞–∫—Ä—ã—Ç–∏—è
    local closeButton = Instance.new("TextButton")
    closeButton.Name = "CloseButton"
    closeButton.Size = UDim2.new(0, 30, 0, 30)
    closeButton.Position = UDim2.new(1, -35, 0, 0)
    closeButton.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
    closeButton.Text = "‚úï"
    closeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    closeButton.Font = Enum.Font.GothamBold
    closeButton.TextSize = 18
    closeButton.Parent = titleLabel
    
    closeButton.MouseButton1Click:Connect(function()
        closeTargetList()
    end)
    
    -- –ö–Ω–æ–ø–∫–∞ "–ê–≤—Ç–æ –≤—ã–±–æ—Ä"
    local autoButton = Instance.new("TextButton")
    autoButton.Name = "AutoButton"
    autoButton.Size = UDim2.new(0.4, 0, 0, 25)
    autoButton.Position = UDim2.new(0.05, 0, 0, 35)
    autoButton.BackgroundColor3 = Color3.fromRGB(60, 180, 60)
    autoButton.Text = "ü§ñ –ê–í–¢–û –í–´–ë–û–†"
    autoButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    autoButton.Font = Enum.Font.GothamBold
    autoButton.TextSize = 11
    autoButton.Parent = targetSelectionFrame
    
    autoButton.MouseButton1Click:Connect(function()
        selectedTarget = nil
        isAutoTarget = true
        print("[Target] –†–µ–∂–∏–º: –ê–≤—Ç–æ (–±–ª–∏–∂–∞–π—à–∏–π –≤—Ä–∞–≥)")
        
        if buttons.pursuit and buttons.pursuit.frame then
            local targetLabel = buttons.pursuit.frame:FindFirstChild("TargetLabel")
            if targetLabel then
                targetLabel.Text = "–¶–µ–ª—å: –ê–í–¢–û (–±–ª–∏–∂–∞–π—à–∏–π –≤—Ä–∞–≥)"
            end
        end
        
        closeTargetList()
    end)
    
    -- –ö–Ω–æ–ø–∫–∞ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è
    local refreshButton = Instance.new("TextButton")
    refreshButton.Name = "RefreshButton"
    refreshButton.Size = UDim2.new(0.4, 0, 0, 25)
    refreshButton.Position = UDim2.new(0.55, 0, 0, 35)
    refreshButton.BackgroundColor3 = Color3.fromRGB(80, 120, 200)
    refreshButton.Text = "üîÑ –û–ë–ù–û–í–ò–¢–¨"
    refreshButton.TextColor3 = Color3.fromRGB(255, 255, 255)
    refreshButton.Font = Enum.Font.GothamBold
    refreshButton.TextSize = 11
    refreshButton.Parent = targetSelectionFrame
    
    refreshButton.MouseButton1Click:Connect(function()
        updateTargetList()
    end)
    
    -- –ü—Ä–æ–∫—Ä—É—á–∏–≤–∞–µ–º—ã–π —Å–ø–∏—Å–æ–∫
    local scrollFrame = Instance.new("ScrollingFrame")
    scrollFrame.Name = "PlayersScroll"
    scrollFrame.Size = UDim2.new(1, -20, 0, 130)
    scrollFrame.Position = UDim2.new(0, 10, 0, 65)
    scrollFrame.BackgroundColor3 = Color3.fromRGB(25, 25, 40)
    scrollFrame.BorderSizePixel = 0
    scrollFrame.ScrollBarThickness = 6
    scrollFrame.AutomaticCanvasSize = Enum.AutomaticSize.Y
    scrollFrame.CanvasSize = UDim2.new(0, 0, 0, 0)
    scrollFrame.Parent = targetSelectionFrame
    
    local scrollCorner = Instance.new("UICorner")
    scrollCorner.CornerRadius = UDim.new(0, 6)
    scrollCorner.Parent = scrollFrame
    
    local listLayout = Instance.new("UIListLayout")
    listLayout.Padding = UDim.new(0, 5)
    listLayout.Parent = scrollFrame
    
    -- –ó–∞–≥—Ä—É–∂–∞–µ–º —Å–ø–∏—Å–æ–∫ –∏–≥—Ä–æ–∫–æ–≤
    updateTargetList()
end

-- –§—É–Ω–∫—Ü–∏—è –¥–ª—è –∑–∞–∫—Ä—ã—Ç–∏—è —Å–ø–∏—Å–∫–∞ –∏–≥—Ä–æ–∫–æ–≤
local function closeTargetList()
    if targetSelectionFrame then
        targetSelectionFrame:Destroy()
        targetSelectionFrame = nil
    end
    isTargetListOpen = false
end

